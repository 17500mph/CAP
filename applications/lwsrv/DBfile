plain300 = (
	Query ADOIsBinaryOK? True;
	FeatureQuery *?Resolution 300dpi;
	FeatureQuery *ColorDevice False;
	FeatureQuery *FaxSupport None;
	FeatureQuery *LanguageLevel '"1"';
	FeatureQuery *TTRasterizer None;
	Query ADOSpooler spooler;
);

